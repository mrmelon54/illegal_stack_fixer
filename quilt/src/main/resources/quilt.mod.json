{
  "schema_version": 1,
  "mixin": [
    "illegal_stack_fixer.mixins.json",
    "illegal_stack_fixer-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "illegal_stack_fixer",
    "version": "${version}",
    "metadata": {
      "name": "Illegal Stack Fixer",
      "description": "Resets stack count to maximum stack size",
      "contributors": {
        "MrMelon54": "Owner"
      },
      "contact": {
        "homepage": "https://mrmelon54.com/minecraft/illegal-stack-fixer",
        "sources": "https://github.com/MrMelon54/illegal_stack_fixer",
        "issues": "https://github.com/MrMelon54/illegal_stack_fixer/issues"
      },
      "license": "GPL-3.0-only",
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.mrmelon54.IllegalStackFixer.quilt.IllegalStackFixerQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20"
      },
      {
        "id": "architectury",
        "version": ">=9.0.0"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}
